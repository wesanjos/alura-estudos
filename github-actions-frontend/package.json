{
  "name": "github-actions-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "next": "^13.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "rm -rf build && npm run build && next export -o build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deploy": "yarn run export && npx vercel ./build --confirm --no-clipboard",
    "deploy:prod": "yarn deploy --prod --token=$VERCEL_TOKEN"
  },
  "devDependencies": {
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8"
  }
}
